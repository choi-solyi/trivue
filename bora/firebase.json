{
  "firestore": {
    "indexes": "firestore.indexes.json",
    "rules": "firestore.rules"
  },
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "public": "dist",
    "rewrites": [
      {
        "destination": "/index.html",
        "source": "**"
      }
    ],
    "site": "borayang"
  },
  "storage": {
    "rules": "storage.rules"
  }
}